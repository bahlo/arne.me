    "lint": "next lint"
  },
  "dependencies": {
    "@types/luxon": "^3.1.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/rss": "^0.0.29",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "gray-matter": "^4.0.3",
    "image-size": "^1.0.2",
    "luxon": "^3.1.0",
    "next": "13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "rss": "^1.2.2",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.18",
    "prettier": "^2.7.1"
  },
  "engines": {
    "node": ">=14.16"
  }
